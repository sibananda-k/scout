<% if user_signed_in? %>
<h1>Welcome Home, <%= current_user.email %></h1>
<%= link_to 'Edit Profile', profile_path, class: 'btn btn-primary' %>

<%= button_to 'Sign out', destroy_user_session_path, method: :delete, class: 'btn btn-danger' %>
<% else %>
<h1>Home</h1>
<%= link_to 'Sign in', new_user_session_path %>
<% end %>